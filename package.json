{
  "name": "app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "rules:dev": "copy /Y firestore.rules.dev firestore.rules",
    "rules:prod": "git restore --source=HEAD --worktree --staged -- firestore.rules",
    "emu:dev": "npm run rules:dev && npx firebase emulators:start --project proyecto-de-graduacion-f1265 --only firestore,auth --import=firebase-export --export-on-exit",
    "emu": "firebase emulators:start --only firestore,auth --import ./.emudata --export-on-exit",
    "emu:fs:dev": "npm run rules:dev && npx firebase emulators:start --project proyecto-de-graduacion-f1265 --only firestore --import=firebase-export --export-on-exit",
    "emu:stop": "firebase emulators:exec --project proyecto-de-graduacion-f1265 --only firestore,auth \"echo stopped\"",
    "seed": "node scripts/seed-all.mjs",
    "fix:assg": "node scripts/fix-assignments.mjs",
    "lint": "echo \"(lint saltado)\"",
    "typecheck": "vue-tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "firebase": "^12.2.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "firebase-admin": "^13.5.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  }
}
